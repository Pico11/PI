<Window x:Class="Indywidualny.Record"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="New user" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox x:Name="Username"></TextBox>
        <TextBlock Grid.Row="1" x:Name="UserCommand"></TextBlock>
        <Button Grid.Row="2" x:Name="Button" Click="Button_OnClick">Begin</Button>
    </Grid>
</Window>
